{
  "name": "promi-codelab-api",
  "version": "1.2.1",
  "description": "NestJS Hexagonal Architecture TypeORM Project",
  "author": "Nathakrit Tato",
  "private": true,
  "license": "Nathakritbc",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "oxlint src apps libs test --fix",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:cov": "vitest run --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/vitest --runInBand",
    "test:e2e": "vitest --config ./test/vitest-e2e.json",
    "release": "semantic-release",
    "prepare": "husky",
    "migration:generate": "npm run build && npx typeorm-ts-node-esm migration:generate -d dist/src/databases/data-source.js",
    "migration:create": "npx typeorm-ts-node-esm migration:create",
    "migration:run": "npm run build && npx typeorm-ts-node-esm migration:run -d dist/src/databases/data-source.js",
    "migration:revert": "npm run build && npx typeorm-ts-node-esm migration:revert -d dist/src/databases/data-source.js",
    "migration:show": "npm run build && npx typeorm-ts-node-esm migration:show -d dist/src/databases/data-source.js",
    "schema:drop": "npm run build && npx typeorm-ts-node-esm schema:drop -d dist/src/databases/data-source.js",
    "schema:sync": "npm run build && npx typeorm-ts-node-esm schema:sync -d dist/src/databases/data-source.js",
    "db:reset": "ts-node -r tsconfig-paths/register src/databases/scripts/reset-database.ts",
    "db:status": "ts-node -r tsconfig-paths/register src/databases/scripts/migration-status.ts",
    "db:stats": "ts-node -r tsconfig-paths/register src/databases/scripts/migration-stats.ts",
    "db:seed:catalog": "ts-node -r tsconfig-paths/register src/databases/scripts/seed-catalog-demo.ts"
  },
  "dependencies": {
    "@nestjs-cls/transactional": "^3.1.0",
    "@nestjs-cls/transactional-adapter-typeorm": "^1.3.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@types/bcrypt": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "argon2": "^0.44.0",
    "builder-pattern": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.1.5",
    "nanoid-dictionary": "^5.0.0",
    "nestjs-cls": "^6.0.1",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "radash": "^12.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0",
    "uuidv4": "^6.2.13",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@semantic-release/npm": "^12.0.2",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/sequelize": "^6.12.0",
    "@types/supertest": "^6.0.2",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "husky": "^9.1.7",
    "oxlint": "^1.11.1",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "semantic-release": "^24.2.7",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "rootDir": "src",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-node"
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}
